<Window x:Class="ImageQualityClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ImageQuality.Client" Width="800" Height="600">
    <Grid>
        <StackPanel Orientation="Vertical" VerticalAlignment="Center">
            <Button Height="30" Margin="5" Width="250" Content="BRISQUE Demo" Click="Brisque_Click" FontSize="18" />
            <Button Height="30" Margin="5" Width="250" Content="Scene Text Demo" Click="SceneText_Click" FontSize="18" />
            <Button Height="30" Margin="5" Width="250" Content="Simple Watermark Demo" Click="SimleWatermark_Click" FontSize="18" />
            <Button Height="30" Margin="5" Width="250" Content="Blur Measure Demo" Click="Blur_Click" FontSize="18" />
            <Button Height="30" Margin="5" Width="250" Content="Intensity Measure Demo" Click="Intensity_Click" FontSize="18" />
            <Separator/>
            <Button Height="30" Margin="5" Width="250" Content="- Bulk Test -" Click="Bulk_Click" FontSize="18" />
        </StackPanel>
    </Grid>
</Window>
